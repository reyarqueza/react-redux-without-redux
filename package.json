{
  "name": "react-webpack-build",
  "version": "1.0.0",
  "description": "Minimum build system for React, Webpack and Express",
  "main": "index.js",
  "scripts": {
    "start": "node ./start-server.js",
    "dev": "npx webpack --watch --config webpack.config.dev.js --mode development",
    "build": "npx webpack --config webpack.config.prod.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reyarqueza/react-webpack-build.git"
  },
