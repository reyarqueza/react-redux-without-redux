  "bugs": {
    "url": "https://github.com/reyarqueza/react-webpack-build/issues"
  },
  "homepage": "https://github.com/reyarqueza/react-webpack-build#readme",
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "babel-loader": "^9.1.3",
    "prettier": "3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
